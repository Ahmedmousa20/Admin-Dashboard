@model Demo.BL.Models.EmployeeVM
@{
    ViewBag.Title = "Delete";
}
<h2>Delete</h2>
<br />


   
<br />
<form class="form-group" asp-controller="Employee" asp-action="Delete">

    <center>
        <a asp-controller="Employee" asp-action="Index" class="btn btn-primary">Back To Data</a>
        <a asp-controller="Employee" asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning">Update</a>
        <input type="submit" value="Delete" class="btn btn-primary" />
        <input type="reset" value="Cancle" class="btn btn-danger" />
    </center>
    <label asp-for="Name">Name</label>
    <input type="text" class="form-control" asp-for="Name" readonly />
    <br />


    <label asp-for="Salary">Salary</label>
    <input type="text" class="form-control" asp-for="Salary" readonly />
    <br />

    <label asp-for="Email">Email</label>
    <input type="text" class="form-control" asp-for="Email" readonly />
    <br />

    <label asp-for="Address">Address</label>
    <input type="text" class="form-control" asp-for="Address" readonly />
    <br />

    <label asp-for="HireDate">Hire Date</label>
    <input type="date" class="form-control" asp-for="HireDate" readonly />
    <br />


    <label asp-for="Notes">Notes</label>
<textarea rows="4" class="form-control" asp-for="Notes" readonly></textarea>
    <br />

    <div class="demo-checkbox">
        <input type="checkbox" asp-for="IsActive" checked="" readonly>
        <label asp-for="IsActive">Is Active</label>
    </div>

    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="PhotoName" />
    <input type="hidden" asp-for="CvName" />
    <br />

    <select asp-for="DepartmentId" asp-items="(SelectList)ViewBag.DepartmentList" readonly>
        <option>Choose Department</option>
    </select>


</form>




